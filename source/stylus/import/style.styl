.tabset
  font-size: 16px
  width: 300px
  padding: 0 0 20px
  .tabs
    height: 39px
    .tab
      float: left
      line-height: 30px
      border: 3px solid black
      padding: 0 10px
      margin: 0 3px 0 0
    .tab-active
      position: relative
  .tabcontentdiv
    border: 3px solid black
    display: none
    padding: 10px
    text-align: center
  .tabcontentdiv-active
    display: block
